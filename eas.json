{
  "cli": {
    "version": ">= 16.19.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "env": {
        "IS_PRODUCTION": "false",
        "BLUETOOTH_ENABLED": "false",
        "APP_ENV": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "env": {
        "IS_PRODUCTION": "false",
        "BLUETOOTH_ENABLED": "false",
        "APP_ENV": "preview"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "IS_PRODUCTION": "true",
        "BLUETOOTH_ENABLED": "true",
        "APP_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
